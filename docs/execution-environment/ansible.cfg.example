[galaxy]
server_list = rh-certified_repo,community_repo,published_repo,community_galaxy
ignore_certs = yes

[galaxy_server.community_repo]
url=https://your-hub-url/api/galaxy/content/community/
token=your-token-here

[galaxy_server.rh-certified_repo]
url=https://your-hub-url/api/galaxy/content/rh-certified/
token=your-token-here

[galaxy_server.published_repo]
url=https://your-hub-url/api/galaxy/content/published/
token=your-token-here

[galaxy_server.community_galaxy]
url=https://galaxy.ansible.com
